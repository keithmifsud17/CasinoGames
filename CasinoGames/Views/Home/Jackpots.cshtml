@model IEnumerable<CasinoGames.Shared.Models.Jackpot>
@{
    ViewData["Title"] = "Jackpots";
}

<div class="text-center">
    <h2>Jackpots</h2>
    <div class="row">
        @foreach (var jackpot in Model)
        {
            <div class="col-sm-6 col-md-4 col-lg-3" style="padding:0.5rem">
                <partial name="_JackpotTile" model="jackpot" />
            </div>
        }
    </div>
</div>